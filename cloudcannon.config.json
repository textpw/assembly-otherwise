    },
    {
      "path": "annotations.md",
      "label": "Annotations Page",
      "fields": [
        { "type": "text", "name": "title", "label": "Page Title" },
        { "type": "markdown", "name": "body", "label": "Annotated Content" }
      ]
    }
  ],
  "data_config": {}
}
